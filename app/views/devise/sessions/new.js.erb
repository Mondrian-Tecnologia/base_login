exibirLogoColorida();
alterarBgSkew();
$('.bg-skew').fadeIn();
// Altera cor do menu para preto
$('.top-menu-item').addClass('secondary');

// ADD MSGs de erro
let message = $("#message .message-body");
message.html(' ');
<% msg = notice || alert %>
<% if msg && msg != t('devise.failure.timeout') && msg != t('devise.failure.unauthenticated') %>
  message.fadeOut('slow', function() {
    message.html("<%= notice || alert %>");
  })
  message.promise().done(function(){
    message.fadeIn('slow');
  })
  <% flash = nil %>
<% else %>
  let body = $('#body-render');
  body.hide();
  body.empty()
  body.append('<%= escape_javascript(render partial: 'new') %>')
  body.fadeIn('slow');
<% end %>

if ( $('.skew.secondary').length === 0 ){ $('.skew').addClass('secondary'); }
portal_init('Entrar');
